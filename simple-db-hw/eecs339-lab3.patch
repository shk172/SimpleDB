+		if((rid.getPageId().pageNumber() != pid.pageNumber()) || (rid.getPageId().getTableId() != pid.getTableId()))
+			throw new DbException("tried to delete entry on invalid page or table");
+		if (!isSlotUsed(rid.tupleno()))
